#!/usr/bin/env bash

here=$(dirname "${0}")
properties="${here}/<%= @properties_filename %>"
jar="${here}/<%= @tsbatch_jar_filename %>"

# Choose the location of the java binary
prefix=
if [ ! "x${JAVA_HOME}" == "x" ]; then
    prefix="${JAVA_HOME}/bin/"
fi

# Run the jar
${prefix}java -jar "${jar}" -c "${properties}" $@

