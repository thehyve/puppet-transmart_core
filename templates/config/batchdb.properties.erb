/* TranSMART batch database properties
 * (Generated by puppet from batchdb.properties.erb)
 */

batch.jdbc.url=<%= scope.lookupvar('::transmart_core::params::db_url') %>
<% case scope.lookupvar('::transmart_core::params::db_type') -%>
<% when 'postgresql' -%>
batch.jdbc.driver=org.postgresql.Driver
<% when 'oracle' -%>
batch.jdbc.driver=oracle.jdbc.driver.OracleDriver
<% else raise "Unknown database type. Check transmart_core::params::db_type" -%>
<% end -%>
batch.jdbc.user=tm_cz
batch.jdbc.password=tm_cz

